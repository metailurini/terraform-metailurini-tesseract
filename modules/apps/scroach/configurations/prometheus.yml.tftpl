global:
  scrape_interval: 1m

scrape_configs:
  - job_name: 'prometheus'
    scrape_interval: 1m
    static_configs:
      - targets: ['localhost:9090']

  - job_name: '${node_exporter_url}'
    static_configs:
      - targets: ['${node_exporter_url}:${node_exporter_port}']

  - job_name: '${postgres_exporter_url}'
    static_configs:
      - targets: ['${postgres_exporter_url}:${postgres_exporter_port}']
